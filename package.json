{
  "name": "relocate-release-tag-actions",
  "description": "GitHub Actions that automatically adds or relocates Git tags (e.g. `v1` and `v1.2`) to your repository when a new release is created with a new Semantic Versioning tag (e.g. `v1.2.3`), using the recommended way on GitHub.",
  "author": "Cheon Park",
  "keywords": [
    "github",
    "actions",
    "release",
    "tag",
    "semver",
    "semantic",
    "versioning"
  ],
  "license": "MIT",
  "repository": "github:say8425/relocate-release-tag-actions",
  "homepage": "https://github.com/say8425/relocate-release-tag-actions",
  "bugs": "https://github.com/say8425/relocate-release-tag-actions/issues",
  "version": "1.0.0",
  "scripts": {
    "lint": "eslint --ext .ts,.json,.cjs . --max-warnings 0",
    "test": "vitest",
    "build": "vite build",
    "check-types": "tsc --noEmit"
  },
  "main": "dist/main.js",
  "packageManager": "pnpm@8.2.0",
  "dependencies": {
    "@actions/core": "^1.10.0",
    "@actions/github": "^5.1.1",
    "semver": "^7.4.0"
  },
  "devDependencies": {
    "@faker-js/faker": "^7.6.0",
    "@octokit/types": "^9.0.0",
    "@types/node": "^18.15.11",
    "@types/semver": "7.3.13",
    "@typescript-eslint/eslint-plugin": "^5.58.0",
    "@typescript-eslint/parser": "^5.58.0",
    "@vercel/ncc": "^0.36.1",
    "@vitest/coverage-c8": "^0.30.1",
    "eslint": "^8.38.0",
    "eslint-config-prettier": "^8.8.0",
    "eslint-import-resolver-typescript": "^3.5.5",
    "eslint-plugin-import": "^2.27.5",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-prefer-arrow": "^1.2.3",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-promise": "^6.1.1",
    "eslint-plugin-simple-import-sort": "^10.0.0",
    "eslint-plugin-unicorn": "^46.0.0",
    "eslint-plugin-vitest": "^0.1.4",
    "prettier": "^2.8.7",
    "typescript": "^5.0.4",
